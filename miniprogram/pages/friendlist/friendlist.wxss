/* pages/friendlist/friendlist.wxss */
.friend_list{
  padding: 30rpx;
}
.page_title{
  font-weight: bold;
  font-size: 32rpx;
}
.list_item{
  padding: 30rpx;
  margin-top: 30rpx;
  display: flex;
  border-radius: 8rpx;
  box-shadow: 0rpx 0rpx 4rpx 2rpx seashell;
}
.list_item image{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}